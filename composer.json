{
  "name": "actb/blade-github-octicons",
  "type": "library",
  "description":"A package to easily make use of Github octicons in your Laravel blade views.",
  "license": "MIT",
  "require": {
      "php": "^7.4|^8.0",
      "blade-ui-kit/blade-icons": "^1.0",
      "illuminate/support": "^8.0|^9.0"
  },
  "require-dev": {
      "orchestra/testbench": "^6.0|^7.0",
      "ext-dom": "*",
      "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Actb\\BladeGithubOcticons\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Actb\\BladeGithubOcticons\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Actb\\BladeGithubOcticons\\GithubOcticonsServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
